CREATE TABLE People(
    id int PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
    name varchar(100) NOT NULL UNIQUE,
    birth DATE
);

CREATE TABLE Books(
    id int PRIMARY KEY GENERATED BY DEFAULT AS IDENTITY,
    person_id int REFERENCES People(id) ON DELETE SET NULL,
    taken_at TIMESTAMP,
    title varchar NOT NULL,
    author varchar,
    year int
);


